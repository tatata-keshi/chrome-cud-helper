{
  "manifest_version": 3,
  "name": "Chrome CUD Helper",
  "version": "1.0.1",
  "description": "開発者向けCUDチェックツール。",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
